{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "outDir": "dist",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2015",
    "lib": ["es2017"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "declaration": true,
    "declarationDir": "dist"
  },
  "include": ["lib"]
}
